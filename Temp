def compare_json(json1: dict, json2: dict) -> str:
    mismatches = []

    # Get union of keys
    all_keys = set(json1.keys()).union(set(json2.keys()))

    for key in all_keys:
        if key not in json1:
            mismatches.append(f"Key '{key}' missing in first JSON.")
        elif key not in json2:
            mismatches.append(f"Key '{key}' missing in second JSON.")
        elif json1[key] != json2[key]:
            mismatches.append(f"Value mismatch at key '{key}': '{json1[key]}' != '{json2[key]}'")

    if not mismatches:
        return "All match"
    else:
        return "\n".join(mismatches)
