from typing import List, Union
from pydantic import BaseModel, Field, RootModel

class SectionExtraction(BaseModel):
    # Accepts either 11 or "page-11" depending on your source
    page: Union[int, str] = Field(...)
    section_heading: str = Field(..., alias="section heading")
    extracted_text: str = Field(..., alias="Extracted text")

    # forbid extra keys; allow dumping with aliases
    model_config = {
        "extra": "forbid",
        "populate_by_name": True,
    }

# If you want the output to be a pure list (like your multi-page example)
class SectionExtractionList(RootModel[List[SectionExtraction]]):
    root: List[SectionExtraction]
    model_config = {"extra": "forbid"}
