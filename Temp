class EncounterPages(BaseModel):
    start_page_number: str = Field(
        ...,
        description="First page of the encounter section."
    )
    end_page_number: str = Field(
        ...,
        description="Last page of the encounter section."
    )

    model_config = {"extra": "forbid"}


class EncounterPagesPayload(BaseModel):
    encounters: List[EncounterPages] = Field(
        ...,
        description="Array of page-number spansâ€”one per encounter."
    )

    model_config = {"extra": "forbid"}
